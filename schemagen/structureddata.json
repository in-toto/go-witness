{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://github.com/in-toto/go-witness/attestation/structureddata/attestor",
  "$ref": "#/$defs/Attestor",
  "$defs": {
    "Attestor": {
      "properties": {
        "documents": {
          "items": {
            "$ref": "#/$defs/DocumentRecord"
          },
          "type": "array"
        },
        "subjectqueries": {
          "additionalProperties": {
            "type": "string"
          },
          "type": "object"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "documents"
      ]
    },
    "DigestSet": {
      "additionalProperties": {
        "type": "string"
      },
      "type": "object"
    },
    "DocumentRecord": {
      "properties": {
        "filename": {
          "type": "string"
        },
        "originaldigest": {
          "$ref": "#/$defs/DigestSet"
        },
        "canonicaldigest": {
          "$ref": "#/$defs/DigestSet"
        },
        "canonical": true
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "filename",
        "originaldigest",
        "canonicaldigest",
        "canonical"
      ]
    }
  }
}